#include<stdio.h>
int main()
{
    int arr[50],size,i,j,temp;
    printf("swati\n");
    printf("enter array size:\n");
    scanf("%d",&size);
    printf("enetr %d array element:\n",size);
    for(i=0;i<size;i++)
    scanf("%d",&arr[i]);
    j=i-1;
    i=0;
    while(i<j)
    {
        temp=arr[i];
        arr[i]=arr[j];
        arr[j]=temp;
        i++;
        j--;
    }
    printf("\nreverse of the array is:\n");
    for(i=0;i<size;i++)
    printf("%d",arr[i]);
    return 0;
}
